name = Birchstrap
description = Base Theme using Bootstrap 2, for Drupal 6
version = 1.0
core = 6.x
engine = phptemplate

stylesheets[all][] = bootstrap/css/bootstrap.min.css
stylesheets[all][] = bootstrap/css/bootstrap-responsive.css
;stylesheets[all][] = bootstrap/css/bootstrap-responsive.v2.1.1.css
stylesheets[all][] = css/style.css
stylesheets[all][] = css/screen_0-480.css
stylesheets[all][] = css/screen_481-767.css
stylesheets[all][] = css/screen_768-979.css
stylesheets[all][] = css/screen_980_and_up.css
stylesheets[print][] = css/print.css

scripts[] = js/jquery.min.js
scripts[] = bootstrap/js/bootstrap.min.js
scripts[] = js/jquery-noconflict.js
;scripts[] = js/html5shiv.js
;scripts[] = js/respond/respond.min.js

regions[header] = Header
regions[navigation] = Navigation
regions[hero] = Hero Unit
regions[left] = Left sidebar
regions[content] = Content
regions[right] = Right sidebar
regions[footer] = Footer